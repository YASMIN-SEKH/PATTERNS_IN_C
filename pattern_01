#include <stdio.h>
#include <stdlib.h>

int main()
{
    printf("pattern 1\n");
    for(int i=1; i<=4; i++) //this loop is used for the number of times you want to run your loop
    {
        for(int j=1; j<=5; j++) //actual loop used for printing pattern
        {
            printf("%d",j);
        }
        printf("\n");
    }


            printf("\n\n");
            printf("pattern 2\n");

     for(int i=1; i<=4; i++) //this loop is used for the number of times you want to run your loop
    {
        for(int j=1; j<=5; j++) //actual loop used for printing pattern
        {
            printf("%d",i);
        }
        printf("\n");
    }


            printf("\n\n");
            printf("pattern 3\n");



    for(int i=1; i<=4; i++) //this loop is used for the number of times you want to run your loop
    {
        for(int j=1; j<=5; j++) //actual loop used for printing pattern
        {
            printf("%c",j+64);
        }
        printf("\n");
    }
    printf("\n\n");
    printf("pattern 4\n");

    for(int i=1; i<=4; i++) //this loop is used for the number of times you want to run your loop
    {
        for(int j=1; j<=5; j++) //actual loop used for printing pattern
        {
            printf("%c",i+64);
        }
        printf("\n");
    }
printf("\n\n");




    return 0;
}
